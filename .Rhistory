minMonteCarlo <- function(f, min, max, maxIter){
i<-0
fmin<-1000000000000
xmin<-min
while(i<maxIter){
xi<-runif(1,min,max);
if(f(xi)<fmin){
fmin=f(xi)
xmin=xi
}
i<-i+1;
}
print(xmin)
print(f(xmin))
}
